<div id="trailcamePane_{{_trailcamId}}">
    <div data-dojo-type="dijit/layout/BorderContainer"
         data-dojo-props="design:'sidebar', gutters:true, liveSplitters:true">
        <div data-dojo-type="dijit/layout/ContentPane"
             data-dojo-props="splitter:false, region:'top'"
             style="width: 100px;">
            <div data-dojo-type="ngw-trailcam/webmap/TrailcamTimeFilter/TrailcamTimeFilter"
                 data-dojo-props="trailcamId:{{_trailcamId}}">
            </div>
            <button data-dojo-type="dijit/form/Button"
                    data-dojo-props="iconClass:'dijitIconEdit', showLabel: true"
                    data-dojo-attach-point="buildReportBtn"
                    type="button">
                {{gettext 'Editor'}}
                <script type="dojo/connect" data-dojo-event="onClick">
                    var win = window.open(ngwConfig.applicationUrl + '/tracker/reports/', '_blank');
                    win.focus();
                </script>
            </button>
        </div>
        <div data-dojo-type="dijit/layout/ContentPane"
             data-dojo-props="splitter:false, region:'center'">
            <div data-dojo-type="ngw-trailcam/webmap/TrailcamItemsPane/TrailcamItemsPane"
                 data-dojo-props="trailcamId:{{_trailcamId}}">
            </div>
        </div>
    </div>
</div>
